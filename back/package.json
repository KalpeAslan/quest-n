{
  "name": "cnlp",
  "version": "1.0.0",
  "description": "AlphaGuilty: Web3 Engagement Ecosystem",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node src/server/index.ts",
    "start:dev": "nodemon --config nodemon.json src/server/index.ts",
    "bot": "nodemon --config nodemon.json src/tg-bot/index.ts",
    "start:debug": "nodemon --config nodemon.json  --exec \"node --require ts-node/register --inspect=9002  src/server/index.ts\"",
    "typeorm": "DB_HOST_MIGRATION=localhost node --require ts-node/register ./node_modules/typeorm/cli.js --config ./src/db/configCli.ts",
    "dev:typeorm": "ts-node ./node_modules/.bin/typeorm --config ./src/db/configCli.ts",
    "typeormTest": "node --require ts-node/register ./node_modules/typeorm/cli.js --config ./test/configCli.ts",
    "admin:dev": "nodemon src/admin/index.ts",
    "admin:debug": "nodemon --exec \"npm run admin:docs && node --require ts-node/register --inspect=9001 src/admin/index.ts\"",
    "admin:dev:start": "npm run admin:docs && ts-node src/admin/index.ts",
    "admin:start": "node dist/admin/index.js",
    "admin:docs": "npx tsoa spec-and-routes -c src/admin/tsoa-admin.json",
    "notify:dev": "nodemon src/notifications/index.ts",
    "test": "ts-node test/population/populateAll.ts && jest --runInBand --silent",
    "test:debug": "NO_LOGS=true jest --runInBand",
    "test:endpoints": "NO_LOGS=true jest test/endpoints --runInBand",
    "test:endpoints:companies": "jest test/endpoints/server/loyalty/loyaltyProject.test.ts --runInBand",
    "test:unit": "NO_LOGS=true jest test/unit --runInBand"
  },
  "nodemonConfig": {
    "ignore": [
      "application_default_credentials.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guiltyalpha/CNLP.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/guiltyalpha/CNLP/issues"
  },
