{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "jsxImportSource": "@emotion/react",
    "noImplicitAny": false,
    "downlevelIteration": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@public/*": ["public/*"],
      "@api": ["src/api"],
      "@api/*": ["src/api/*"],
      "@assets": ["src/assets"],
      "@assets/*": ["src/assets/*"],
      "@services": ["src/services"],
      "@services/*": ["src/services/*"],
      "@components": ["src/components"],
      "@components/*": ["src/components/*"],
      "@pages": ["src/pages"],
      "@pages/*": ["src/pages/*"],
      "@models": ["src/models"],
      "@models/*": ["src/models/*"],
      "@modules/*": ["src/modules/*"],
      "@styles/*": ["src/styles/*"],
      "@store": ["src/store"],
      "@store/*": ["src/store/*"],
      "@context": ["src/context"],
      "@context/*": ["src/context/*"],
      "@hooks": ["src/hooks"],
      "@hooks/*": ["src/hooks/*"],
      "@locales/*": ["src/locales/*"],
      "@utils": ["utils"],
      "@utils/*": ["utils/*"],
      "@queries": ["src/modules/blog/queries"],
      "@queries": ["src/modules/blog/queries/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "lingui.config.js"],
  "exclude": ["node_modules"]
}
